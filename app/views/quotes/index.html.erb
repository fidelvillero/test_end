<h1>Listing quotes</h1>

<table>
  <tr>
    <th>Init</th>
    <th>End</th>
    <th>State</th>
    <th>Id doctors</th>
    <th>Id user</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quotes.each do |quote| %>
  <tr>
    <td><%= quote.init %></td>
    <td><%= quote.end %></td>
    <td><%= quote.state %></td>
    <td><%= quote.id_doctors %></td>
    <td><%= quote.id_user %></td>
    <td><%= link_to 'Show', quote %></td>
    <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
    <td><%= link_to 'Destroy', quote, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Quote', new_quote_path %>
