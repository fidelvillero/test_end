<h1>Listing quotes</h1>

<table>
  <tr>
    <th>Init</th>
    <th>End</th>
	<th>date_init</th>
    <th>State</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quotes.each do |quote| %>
  <tr>
    <td><%= quote.hour_init %></td>
    <td><%= quote.hour_end %></td>
    <td><%= quote.state %></td>
	<td><%= quote.date_init %></td>
	<td></td>
	<td></td>
	<td></td>
    <td><%= link_to 'Show', quote %></td>
    <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
	<% if current_user.eql?("Admin")%>
    	<td><%= link_to 'Destroy', quote, :confirm => 'Are you sure?', :method => :delete %></td>
	<% end%>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Quote', new_quote_path %>
